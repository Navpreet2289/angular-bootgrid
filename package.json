{
  "name": "angular-bootgrid",
  "version": "1.0.0-alpha.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hjalmers/angular-bootgrid.git"
  },
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "copy:scss": "copyfiles -f src/bootgrid/bootgrid.scss bootgrid/scss",
    "clean": "rimraf bootgrid \"index.@(js|js.map|d.ts|metadata.json|ngsummary.json)\"",
    "clean:factory": "rimraf \"bootgrid/**/*.ngfactory.ts\"",
    "build": "ngc -p src/tsconfig-aot.json && npm run copy:scss",
    "postbuild": "npm run -s clean:factory",
    "prepublish": "npm run -s build",
    "postpublish": "npm run -s clean",
    "e2e": "protractor"
  },
  "private": false,
  "main": "index.js",
  "dependencies": {
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/forms": "^4.1.0",
    "@angular/http": "^4.1.0",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/router": "^4.1.0",
    "angular-resizable-element": "^1.0.0",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "dragula": "^3.7.2",
    "ng2-dragula": "^1.2.2",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^4.1.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "angular-exemplify": "^2.0.0",
    "autoprefixer": "^6.6.1",
    "clean-css": "^4.0.12",
    "codelyzer": "~3.0.1",
    "copyfiles": "^1.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.3.0",
    "postcss-cli": "^2.6.0",
    "prismjs": "^1.6.0",
    "protractor": "~5.1.0",
    "renamer": "^0.6.1",
    "rimraf": "^2.5.4",
    "ts-node": "~2.0.0",
    "tslint": "4.3.0",
    "typescript": "~2.2.0"
  }
}
