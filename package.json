{
  "name": "angular-bootgrid",
  "version": "1.0.0-alpha.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hjalmers/angular-bootgrid.git"
  },
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "copy:scss": "copyfiles -f src/bootgrid/bootgrid.scss bootgrid/scss",
    "clean": "rimraf bootgrid \"index.@(js|js.map|d.ts|metadata.json|ngsummary.json)\"",
    "clean:factory": "rimraf \"bootgrid/**/*.ngfactory.ts\"",
    "build": "ngc -p src/tsconfig-aot.json && npm run copy:scss",
    "postbuild": "npm run -s clean:factory",
    "prepublish": "npm run -s build",
    "postpublish": "npm run -s clean",
    "e2e": "protractor"
  },
  "private": true,
  "main": "index.js",
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/compiler-cli": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "angular-resizable-element": "^0.5.7",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "dragula": "^3.7.2",
    "ng2-dragula": "^1.2.2",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.26",
    "angular-exemplify": "^1.1.0",
    "autoprefixer": "^6.6.1",
    "clean-css": "^3.4.24",
    "codelyzer": "~2.0.0-beta.1",
    "copyfiles": "^1.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "node-sass": "^4.3.0",
    "postcss-cli": "^2.6.0",
    "prismjs": "^1.6.0",
    "protractor": "4.0.13",
    "renamer": "^0.6.1",
    "rimraf": "^2.5.4",
    "ts-node": "1.2.1",
    "tslint": "4.3.0",
    "typescript": "~2.0.3",
    "webdriver-manager": "10.2.5"
  }
}
