{
  "name": "angular-bootgrid",
  "version": "1.0.0-alpha.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hjalmers/angular-bootgrid.git"
  },
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "hmr": "ng serve --hmr -e=hmr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pree2e": "webdriver-manager update",
    "copy:scss": "copyfiles -f src/bootgrid/bootgrid.scss bootgrid/scss",
    "clean": "rimraf bootgrid \"index.@(js|js.map|d.ts|metadata.json|ngsummary.json)\"",
    "clean:factory": "rimraf \"bootgrid/**/*.ngfactory.ts\"",
    "compile": "ngc -p src/tsconfig.publish.json && npm run copy:scss",
    "postcompile": "npm run -s clean:factory",
    "prepublishOnly": "npm run -s compile",
    "postpublish": "npm run -s clean"
  },
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "index.js",
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "angular-resizable-element": "^0.5.7",
    "bootstrap": "^4.0.0-beta",
    "core-js": "^2.4.1",
    "dragula": "^3.7.2",
    "ng2-dragula": "^1.5.0",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-generic-table/core": "^4.6.0",
    "@angular/cli": "1.3.2",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@angularclass/hmr": "^2.1.3",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "angular-exemplify": "^2.0.1",
    "angular2-highcharts": "^0.5.5",
    "autoprefixer": "^6.6.1",
    "clean-css": "^3.4.24",
    "codelyzer": "~3.1.1",
    "copyfiles": "^1.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.3.0",
    "postcss-cli": "^2.6.0",
    "prismjs": "^1.6.0",
    "protractor": "~5.1.2",
    "renamer": "^0.6.1",
    "rimraf": "^2.5.4",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "webdriver-manager": "10.2.5"
  }
}
